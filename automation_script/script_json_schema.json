                    "type": "integer"
                  },
                  "right_arm_item_id": {
                    "type": "integer"
                  },
                  "foot_item_id": {
                    "type": "integer"
                  },
                  "animal_item_id": {
                    "type": "integer"
                  },
                  "belt_item_id": {
                    "type": "integer"
                  },
                  "pants_item_id": {
                    "type": "integer"
                  },
                  "yield_item_id": {
                    "type": "integer"
                  }
                }
              },
              "work_equipment": {
                "type": "object",
                "properties": {
                  "head_item_id": {
                    "type": "integer"
                  },
                  "neck_item_id": {
                    "type": "integer"
                  },
                  "left_arm_item_id": {
                    "type": "integer"
                  },
                  "body_item_id": {
                    "type": "integer"
                  },
                  "right_arm_item_id": {
                    "type": "integer"
                  },
                  "foot_item_id": {
                    "type": "integer"
                  },
                  "animal_item_id": {
                    "type": "integer"
                  },
                  "belt_item_id": {
                    "type": "integer"
