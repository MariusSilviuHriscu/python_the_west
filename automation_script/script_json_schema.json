              "target_product_id",
              "target_desired_job_id",
              "motivation_consumable",
              "energy_consumable",
              "luck_bonus_consumable",
              "product_equipment",
              "work_equipment",
              "seconds_flag"
            ]
          }
        }
      },
      "required": ["name", "password", "worlds"]
    }
